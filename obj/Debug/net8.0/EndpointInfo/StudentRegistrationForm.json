                  "PhotoFile": {
                    "type": "string",
                    "format": "binary"
                  },
                  "SignatureFile": {
                    "type": "string",
                    "format": "binary"
                  },
                  "CitizenshipFile": {
                    "type": "string",
                    "format": "binary"
                  },
                  "CharacterCertificateFile": {
                    "type": "string",
                    "format": "binary"
                  },
                  "MarksheetFiles": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              },
              "encoding": {
                "PhotoFile": {
                  "style": "form"
                },
                "SignatureFile": {
                  "style": "form"
                },
                "CitizenshipFile": {
                  "style": "form"
                },
                "CharacterCertificateFile": {
                  "style": "form"
                },
                "MarksheetFiles": {
                  "style": "form"
                }
